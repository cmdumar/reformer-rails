<h1>Edit your user details</h1>

<%= form_for @user do |f| %>
  <%= f.label :username, "Enter your username" %>
  <%= f.text_field :username %>
  <%= f.label :email, "Enter your email" %>
  <%= f.email_field :email %>
  <%= f.label :password, "Enter your password" %>
  <%= f.password_field :password %>
  <%= f.submit 'Create' %>
<% end %>
